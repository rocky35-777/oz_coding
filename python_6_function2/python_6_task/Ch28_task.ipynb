   "cell_type": "code",
   "source": [
    "def min_max(*numbers):\n",
    "    num_tuple = tuple(numbers)\n",
    "    print(\"최소값:\", min(num_tuple), \"최대값:\", max(num_tuple))\n",
    "\n",
    "min_max(10, 4, 6)\n"
